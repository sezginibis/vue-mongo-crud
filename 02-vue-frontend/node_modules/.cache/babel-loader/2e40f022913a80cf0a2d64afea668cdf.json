{"ast":null,"code":"import PostTitle from '@/components/posts/PostTitle.vue';\nimport { onBeforeMount, ref, computed } from 'vue';\nimport { useRoute } from 'vue-router';\nexport default {\n  name: 'PostId',\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const route = useRoute();\n    const post = ref([]);\n    const title = ref('This is an one post list page');\n    const deneme = computed(() => {\n      if (route.params.id !== post.value._id) {\n        return true;\n      }\n    });\n    onBeforeMount(async () => {\n      try {\n        await fetch('http://127.0.0.1:5000/post/' + route.params.id).then(response => response.json()).then(json => post.value = json);\n      } catch (error) {\n        console.log(error);\n      }\n    });\n    const __returned__ = {\n      route,\n      post,\n      title,\n      deneme,\n      PostTitle,\n      onBeforeMount,\n      ref,\n      computed,\n      useRoute\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AACA,OAAOA,SAAP,MAAsB,kCAAtB;AACA,SAASC,aAAT,EAAwBC,GAAxB,EAA6BC,QAA7B,QAA6C,KAA7C;AACA,SAASC,QAAT,QAAyB,YAAzB;;;;;;;;IACA,MAAMC,KAAK,GAAGD,QAAQ,EAAtB;IAEA,MAAME,IAAI,GAAGJ,GAAG,CAAC,EAAD,CAAhB;IACA,MAAMK,KAAK,GAAGL,GAAG,CAAC,+BAAD,CAAjB;IACA,MAAMM,MAAM,GAAGL,QAAQ,CAAC,MAAM;MAC1B,IAAGE,KAAK,CAACI,MAAN,CAAaC,EAAb,KAAoBJ,IAAI,CAACK,KAAL,CAAWC,GAAlC,EAAuC;QACnC,OAAO,IAAP;MACH;IACJ,CAJsB,CAAvB;IAMAX,aAAa,CAAC,YAAY;MACtB,IAAI;QACA,MAAMY,KAAK,CAAC,gCAAgCR,KAAK,CAACI,MAAN,CAAaC,EAA9C,CAAL,CACDI,IADC,CACIC,QAAQ,IAAIA,QAAQ,CAACC,IAAT,EADhB,EAEDF,IAFC,CAEIE,IAAI,IAAIV,IAAI,CAACK,KAAL,GAAaK,IAFzB,CAAN;MAGH,CAJD,CAIE,OAAOC,KAAP,EAAc;QACZC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACH;IACJ,CARY,CAAb","names":["PostTitle","onBeforeMount","ref","computed","useRoute","route","post","title","deneme","params","id","value","_id","fetch","then","response","json","error","console","log"],"sources":[